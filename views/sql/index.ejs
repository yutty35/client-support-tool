<!--
	Return to Home
-->
<style>
.homeButton {
	width: 60px;
	position:fixed;
	z-index:1;
	top: 0%;
	right: 0px;
}
</style>

<script nonce=<%- nonce %>>
	window.addEventListener("load", () => {
		document.getElementById("return_to_home").addEventListener("click", (e) => {
			redirectTo("/");
		});
	});
	
	const redirectTo = (url) => {
		location.href = url;
	}
</script>

<div class="homeButton">
	<button class="ds-button _ds-flat _ds-basic _ds-bg-contextual-blue-3" id="return_to_home">
	<span class="ds-icon-home ds-icon-large ds-text-neutral-1">
	</span>
	</button>
</div>

<form>

<div class="ds-button-group-h ds-row ds-mar-0 ds-pad-0 ds-mar-t-1">
	<button class="ds-button ds-basic ds-col-3 ds-icon-search" formMethod="get" formAction="/sql/sql_custom">
	Custom SQL
	</button>

	<button class="ds-button ds-basic ds-col-3 ds-icon-search" formMethod="get" formAction="/sql/sql_ch86user">
